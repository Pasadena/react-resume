{
  "name": "react-resume",
  "version": "1.0.0",
  "description": "React resume",
  "main": "lib/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental",
    "heroku": "NODE_ENV=production node server/server.js",
    "postinstall": "webpack -p --config ./webpack.config.dev.js"
  },
  "author": "Simo-Pekka Koskinen",
  "license": "MIT",
  "devDependencies": {
  },
  "dependencies": {
    "nodemon": "^1.3.7",
    "webpack": "^1.9.10",
    "babel": "^5.5.6",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "express": "^4.12.4",
    "jade": "^1.10.0",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "css-loader": "^0.15.1",
    "style-loader": "^0.12.3",
    "file-loader": "^0.8.4",
    "react-hot-loader": "^1.2.7",
    "webpack-dev-server": "^1.9.0"
  }
